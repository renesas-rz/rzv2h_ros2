+TOPI_DEFINE_BCAST_OP(trunc_mod, {
+  if (a.dtype().is_int() || a.dtype().is_uint()) {
+    return truncmod(a, b);
+  } else {
+    return a - trunc_divide(a, b) * b;
+  }
+});
