+/*!
+ * \file tvm/ir/module.h
+ * \brief IRModule that holds the functions and type definitions.
+ */
+#ifndef TVM_IR_MODULE_H_
+#define TVM_IR_MODULE_H_
+
+#include <tvm/ir/adt.h>
+#include <tvm/ir/expr.h>
+#include <tvm/ir/function.h>
