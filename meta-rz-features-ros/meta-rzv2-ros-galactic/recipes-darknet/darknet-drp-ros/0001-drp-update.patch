+/*!
+ * \file tvm/tir/expr_functor.h
+ *
+ * \brief Functors for tir expressions.
+ */
+#ifndef TVM_TIR_EXPR_FUNCTOR_H_
+#define TVM_TIR_EXPR_FUNCTOR_H_
+
+#include <tvm/node/functor.h>
+#include <tvm/tir/expr.h>
