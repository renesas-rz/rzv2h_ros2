+/*!
+ * \file tvm/runtime/container/optional.h
+ * \brief Runtime Optional container types.
+ */
+#ifndef TVM_RUNTIME_CONTAINER_OPTIONAL_H_
+#define TVM_RUNTIME_CONTAINER_OPTIONAL_H_
+
+#include <utility>
+
+#include "./base.h"
