+class LogMessage {
+ public:
+  LogMessage(const std::string& file, int lineno, int level) {
+    std::time_t t = std::time(nullptr);
+    stream_ << "[" << std::put_time(std::localtime(&t), "%H:%M:%S") << "] " << file << ":" << lineno
+            << level_strings_[level];
