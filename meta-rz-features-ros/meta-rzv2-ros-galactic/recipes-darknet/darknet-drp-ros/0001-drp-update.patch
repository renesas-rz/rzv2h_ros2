+RefRead WithFields(RefRead ref_read, Optional<Expr> opt_ref = Optional<Expr>(),
+                   Optional<VirtualDevice> opt_virtual_device = Optional<VirtualDevice>(),
+                   Optional<Span> opt_span = Optional<Span>());
