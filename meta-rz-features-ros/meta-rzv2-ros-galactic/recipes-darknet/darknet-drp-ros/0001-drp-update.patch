+template <>
+struct typed_packed_call_dispatcher<void> {
+  template <typename... Args>
+  TVM_ALWAYS_INLINE static void run(const PackedFunc& pf, Args&&... args) {
+    pf(std::forward<Args>(args)...);
+  }
+};
