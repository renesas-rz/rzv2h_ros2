+    return f_build(build_inputs);
+  }
+
+  static constexpr const char* _type_key = "meta_schedule.PyBuilder";
+  TVM_DECLARE_FINAL_OBJECT_INFO(PyBuilderNode, BuilderNode);
+};
+
