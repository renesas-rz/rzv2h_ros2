+            drpai_obj_info.drpai_address.aimac_cmd_addr = l_addr;
+            drpai_obj_info.drpai_address.aimac_cmd_size = l_size;
+        }
+        else
+        {
+            /*Ignore other space*/
+        }
+
+    }
+
+    ifs.close();
+    return PRE_SUCCESS;
+}
